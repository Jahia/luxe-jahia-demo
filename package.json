{
  "name": "luxe-jahia-demo",
  "version": "0.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=development && jahia-pack",
    "build:production": "webpack --mode=production && jahia-pack",
    "clean": "rm -rf dist/",
    "deploy": "jahia-deploy",
    "watch": "webpack --mode=development --watch",
    "lint": "eslint .",
    "test": "yarn lint"
  },
  "jahia": {
    "snapshot": true,
    "required-version": "8.2.0.0-SNAPSHOT",
    "module-type": "templatesSet",
    "module-dependencies": "default",
    "static-resources": "/javascript,/client",
    "server": "main.js",
    "maven": {
      "groupId": "org.jahia.modules.npm",
      "distributionManagement": {
        "repository": {
          "id": "jahia-releases",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-releases"
        },
        "snapshotRepository": {
          "id": "jahia-snapshots",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-snapshots"
        }
      }
    }
  },
  "dependencies": {
    "@jahia/js-server-core": "^0.0.15",
    "bootstrap": "^5.3.3",
    "graphql": "^16.7.1",
    "i18next": "^23.10.1",
    "react": "^18.2.0",
    "react-i18next": "^14.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@cyclonedx/webpack-plugin": "^3.12.0",
    "@heroicons/react": "^2.1.1",
    "@jahia/eslint-config": "^2.1.2",
    "@jahia/scripts": "^1.3.5",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "clsx": "^2.1.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.9.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "extra-watch-webpack-plugin": "^1.0.3",
    "mini-css-extract-plugin": "^2.7.7",
    "prop-types": "^15.8.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "style-loader": "^3.3.4",
    "styled-jsx": "^5.1.2",
    "typescript": "^5.4.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "packageManager": "yarn@4.3.1"
}
