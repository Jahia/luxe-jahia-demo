import React from 'react';
import {getNodeProps, Render, useServerContext} from "@jahia/js-server-core";

export default FormDefault = () => {
    const {renderContext, currentNode} = useServerContext();
    const form = getNodeProps(currentNode, ['formType','formView']);

    return(
        <Render node={currentNode} view={form.formView || 'default'}
    );
}

FormDefault.jahiaComponent = {
    nodeType: 'luxe:form',
    name: 'default',
    componentType: 'view'
};
